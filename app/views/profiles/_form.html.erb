            <%= form_for @profile ,:html => {:class => "form-horizontal form-without-legend" } do |f| %>
  <% if @profile.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@profile.errors.count, "error") %> prohibited this profile from being saved:</h2>

      <ul>
      <% @profile.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <br>
  <div class="well">
  <h2> ایجاد پروفایل </h2>
   
   <div class="form-group">
      <%= f.label :نام ,class:'col-lg-2 control-label ' , style:'float:right;'%> 

          <div class="col-lg-8">
      <%= f.text_field :firstname ,class:'form-control' , style:'margin-top:19px' %>
    </div>
  </div> 

  <div class="form-group">
    <label class="col-lg-2 control-label" , style="float:right;" >نام خانوادگی</label><br>
      <div class="col-lg-8">
    <%= f.text_field :lastname ,class:'form-control' %>
      </div>
  </div>
  <div class="form-group">
    <%= f.label :ایمیل , class:'col-lg-2 control-label' , style:'float:right;'%> <span class="require">*</span><br>
    <div class="col-lg-8">
    <%= f.text_field :email , class:'form-control'  %>
      </div>
  </div>
  <div class="form-group">
    <%= f.label :سن , class:'col-lg-2 control-label' , style:'float:right;'%> <span class="require">*</span><br>
    <div class="col-lg-8">
    <%= f.number_field :age ,class:'form-control mytest' ,style:'padding-top:0px; padding-bottom:0px;' %>
      </div>
  </div>
  <div class="form-group">
    <%= f.label :جنسیت ,class:'col-lg-2 control-label' , style:'float:right;'%><br>
    <div class="col-lg-8 checkbox-list">
      <label>
        <input type="checkbox" name="gender_male"> مرد
      </label>
      <label>
        <input type="checkbox" name="gender_female"> زن
      </label>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :قد ,class:'col-lg-2 control-label' , style:'float:right;'%><span class="require">*</span><br>
      <div class="col-lg-8">
    <%= f.number_field :height,class:'form-control',style:'padding-top:0px; padding-bottom:0px;' %>
      </div>
  </div>
  <div class="form-group">
    <%= f.label :وزن ,class:'col-lg-2 control-label' , style:'float:right;' %> <span class="require">*</span><br>
    <div class="col-lg-8">
    <%= f.number_field :weight ,class:'form-control',style:'padding-top:0px; padding-bottom:0px;' %>
      </div>
  </div>
  <div class="form-group">
    <% f.label :user_id %>
    <% f.number_field :user_id %>
  </div>
  <div class="row">
    <div class="col-lg-8 col-md-offset-2 padding-left-0 padding-top-20">
      <button class="btn btn-primary btn_cls" type="submit"> ذخیره</button>
    </div>
  </div>

